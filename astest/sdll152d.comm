                        FONCTION=FREQ1_AE,),
              )



SPECTRE2=DEFI_SPEC_TURB(
              SPEC_EXCI_POINT=_F(
                   GRAPPE_2 = 'ASC_EXC',
                   RHO_FLUI = 1000.,
                   CARA_ELEM = CARA,
                   MODELE = MODEL,
                   GROUP_NO = 'GN100',)
)

SPRJ2=PROJ_SPEC_BASE(
                SPEC_TURB=(SPECTRE2, ),
                TOUT_CMP='NON',
                TOUT='OUI',
                NB_POIN=1024,
                FREQ_INIT= 1. ,
                FREQ_FIN= 500. ,
                BASE_ELAS_FLUI=MELES2,
                VITE_FLUI=vite,
                OPTION='TOUT',
               )

RPM02=DYNA_SPEC_MODAL(
          BASE_ELAS_FLUI=MELES2,
                VITE_FLUI=vite,
                   EXCIT=_F(  INTE_SPEC_GENE = SPRJ2,),
                OPTION='TOUT' 
               )

