                      NUME_DDL=NDDL[j]
                     );


#Gyroscopie incluse
Ge = [None]*nbT
G = [None]*nbT
RGe=[None]*nbT
RG=[None]*nbT
GOM=[None]*nbT


for j,NTRIM in enumerate(tours):
        FREQ=NTRIM/60.
        OM=(FREQ * 2) * NP.pi

        Ge[j]=CALC_MATR_ELEM(OPTION='MECA_GYRO',
                      MODELE=MODELE,
                      CHAM_MATER=CHMAT,
                      CARA_ELEM=CEK[j],
                      CHARGE=BCR,
                     );

        G[j]=ASSE_MATRICE(MATR_ELEM=Ge[j],NUME_DDL=NDDL[j],);

#raideur gyroscopique (vitesse variable uniquement)

        RGe[j]=CALC_MATR_ELEM(OPTION='RIGI_GYRO',
                                MODELE=MODELE,
                                CHAM_MATER=CHMAT,
                                CARA_ELEM=CEK[j],);

        RG[j]=ASSE_MATRICE(MATR_ELEM=RGe[j],NUME_DDL=NDDL[j]);


GOM=[None]*nbT

for j,NTRIM in enumerate(tours):
        FREQ=NTRIM/60.
        OM=(FREQ * 2) * NP.pi

        GOM[j]=COMB_MATR_ASSE(COMB_R=(
