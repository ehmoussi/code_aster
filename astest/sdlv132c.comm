Ae = [None]*nbT
A = [None]*nbT

for j,NTRIM in enumerate(tours):
        FREQ=NTRIM/60.
        OM=(FREQ * 2) * NP.pi


# MATRICE AMORTISSEMENT : CEK
        Ae[j]=CALC_MATR_ELEM(OPTION='AMOR_MECA',
                      MODELE=MODELE,
                      CHAM_MATER=CHMAT,
                      CARA_ELEM=CEK[j],
                      RIGI_MECA=Ke[j],
                      MASS_MECA=Me[j],
                     );


