                      CARA_ELEM=CEK[j],
                      CHARGE=BCR,
                     );


    NDDL[j]=NUME_DDL(MATR_RIGI=Ke[j]);


    K[j]=ASSE_MATRICE(MATR_ELEM=Ke[j],
                      NUME_DDL=NDDL[j]
                     );


    Me[j]=CALC_MATR_ELEM(OPTION='MASS_MECA',
                      MODELE=MODELE,
                      CHAM_MATER=CHMAT,
                      CARA_ELEM=CEK[j],
                      CHARGE=BCR,
                     );


