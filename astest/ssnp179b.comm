########################################################################
#          TEST DE VERIFICATION EN PLASTICITE INCOMPRESSIBLE           #
#                       MEMBRANE DE COOK                               #
########################################################################

DEBUT( CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'),)


mesh = LIRE_MAILLAGE(FORMAT='MED')

mesh = MODI_MAILLAGE(reuse=mesh, MAILLAGE=mesh,
                     ORIE_PEAU_2D=_F(GROUP_MA=('Encast', 'load',),),)

model = AFFE_MODELE(
    AFFE=_F(MODELISATION=('D_PLAN_INCO_UPG', ),
            PHENOMENE='MECANIQUE', TOUT='OUI'),
    MAILLAGE=mesh
)

ctrac = LIRE_FONCTION(UNITE=21,
                      NOM_PARA='EPSI',
                      PROL_DROITE='LINEAIRE',)

mater = DEFI_MATERIAU(
    TRACTION=_F(SIGM=ctrac,), ELAS=_F(E=206.9, NU=0.29)
)

fieldmat = AFFE_MATERIAU(
    AFFE=_F(MATER=(mater, ), TOUT='OUI'), MAILLAGE=mesh, MODELE=model
)

rampe = DEFI_FONCTION(NOM_PARA='INST', VALE=(
    0.0, 0.0, 1.0, 1.0), PROL_DROITE='LINEAIRE', PROL_GAUCHE='EXCLU',)

listr = DEFI_LIST_REEL(DEBUT=0.0, INTERVALLE=(
    _F(JUSQU_A=0.1, NOMBRE=1),
    _F(JUSQU_A=0.5, NOMBRE=3),
    _F(JUSQU_A=0.8, NOMBRE=3),
    _F(JUSQU_A=1.0, NOMBRE=4),
),)

times = DEFI_LIST_INST(
    DEFI_LIST=_F(LIST_INST=listr),
    ECHEC=_F(ACTION='DECOUPE', EVENEMENT='ERREUR', SUBD_NIVEAU=3)
)

depl = AFFE_CHAR_CINE(
    MECA_IMPO=_F(DX=0.0, DY=0.0, GROUP_MA=('Encast', )), MODELE=model, INFO=1,
)

FY = FORMULE(NOM_PARA=('X', 'Y', 'INST'), VALE='0.3125')

load = AFFE_CHAR_MECA_F(
    FORCE_CONTOUR=_F(FY=FY, GROUP_MA=('load', )), MODELE=model
)

resnonl = STAT_NON_LINE(
    CHAM_MATER=fieldmat,
    COMPORTEMENT=_F(DEFORMATION='GDEF_LOG',
                    RELATION='VMIS_ISOT_TRAC', TOUT='OUI'),
    CONVERGENCE=_F(ITER_GLOB_MAXI=10, RESI_GLOB_RELA=1e-6),
    EXCIT=(
        _F(CHARGE=depl, TYPE_CHARGE='FIXE_CSTE'),
        _F(CHARGE=load, FONC_MULT=rampe,
           TYPE_CHARGE='FIXE_CSTE'),
    ),
    INCREMENT=_F(LIST_INST=times),
    MODELE=model,
    NEWTON=_F(MATRICE='TANGENTE', PREDICTION='TANGENTE', REAC_ITER=1,),
    SOLVEUR=_F(METHODE='MUMPS'),
    INFO=1,
)


resnonl = CALC_CHAMP(
    reuse=resnonl,
    CHAM_MATER=fieldmat,
    CRITERES=('SIEQ_NOEU',),
    MODELE=model,
    RESULTAT=resnonl,
)


TEST_RESU(RESU=_F(INST=1.0,
                  GROUP_NO=('A', ),
                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=resnonl,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=7.004129190541177,
                  VALE_REFE=7.00031,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  GROUP_NO=('A', ),
                  REFERENCE='AUTRE_ASTER',
                  RESULTAT=resnonl,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=-6.250004304512107,
                  VALE_REFE=-6.23907,
                  CRITERE='RELATIF',
                  PRECISION=1.E-2,),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  GROUP_NO=('A', ),
                  RESULTAT=resnonl,
                  NOM_CHAM='SIEQ_NOEU',
                  NOM_CMP='TRSIG',
                  VALE_CALC=-0.12764345352670564,
                  CRITERE='RELATIF',),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  GROUP_NO=('C', ),
                  RESULTAT=resnonl,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=1.4844671710323847,
                  CRITERE='RELATIF',),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  GROUP_NO=('C', ),
                  RESULTAT=resnonl,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=-0.21178479002911285,
                  CRITERE='RELATIF',),
          )

TEST_RESU(RESU=_F(INST=1.0,
                  GROUP_NO=('C', ),
                  RESULTAT=resnonl,
                  NOM_CHAM='SIEQ_NOEU',
                  NOM_CMP='TRSIG',
                  VALE_CALC=0.8240497101313431,
                  CRITERE='RELATIF',),
          )

#IMPR_RESU(FORMAT='MED', RESU=_F( RESULTAT=resnonl), UNITE=80)


FIN()
