           VALE_CALC=1.57073966912E-05,
           VALE_REFE=1.721E-05,
           NOM_PARA='G',
           TABLE=GLOC1,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=2.66827E+01,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.14000000000000001,
           VALE_CALC=1.60124079793E-05 ,
           VALE_REFE=1.721E-05,
           NOM_PARA='G',
           TABLE=GLOC2,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=2.66827E+01,),
           )

#----------------------------------------------------------------------
#              TEST DE G(C) SUR LES 3 COURONNES
#----------------------------------------------------------------------

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC=4.99174197815E-05,
           VALE_REFE=5.215E-05,
           NOM_PARA='G',
           TABLE=GLOC0,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=1.78016E+01,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC=5.12383113014E-05,
           VALE_REFE=5.215E-05,
           NOM_PARA='G',
           TABLE=GLOC1,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=1.78016E+01,),
           )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.050000000000000003,
           VALE_CALC=5.01341793449E-05,
           VALE_REFE=5.215E-05,
           NOM_PARA='G',
           TABLE=GLOC2,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=1.78016E+01,),
           )
FIN()
