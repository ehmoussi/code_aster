#----------------------------------------------------------------------

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=0.14000000000000001,
           VALE_CALC=1.48288183146E-05,
           VALE_REFE=1.721E-05,
           NOM_PARA='G',
           TABLE=GLOC0,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
