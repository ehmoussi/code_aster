#########################################################################
# VALIDATION DE CALC_CHAMP / SIRO_ELEM
#########################################################################
# PRINCIPE DU TEST :
# ON IMPOSE UNE DEPLACEMENT IMPOSE AFFINE SUR LES FACES D'UN BLOC
# LE CHAMP DE CONTRAINTES OBTENU EST ALORS HOMOGENE (ET CONNU).
# ON PEUT ALORS CALCULER ANALYTIQUEMENT LES CONTRAINTES DE SIRO_ELEM.
#########################################################################

DEBUT(  CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
MA = LIRE_MAILLAGE(FORMAT='MED',INFO=2,
                   UNITE=20,
                   )
# ORIENTATION DES FACES 'FAC1' et 'FAC2' :
MA=MODI_MAILLAGE(reuse=MA, MAILLAGE=MA,
                 ORIE_PEAU_2D=(
                     _F(GROUP_MA=('UP','DOWN','LEFT','RIGHT'),  GROUP_MA_SURF=('FAC1')),
                     ))


MO=AFFE_MODELE(   MAILLAGE=MA,
           AFFE=_F( TOUT = 'OUI',  PHENOMENE = 'MECANIQUE', MODELISATION = 'D_PLAN'))


MAT=DEFI_MATERIAU(   ELAS=_F( E = 1.E+05,  NU = 0.0,))

CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MA,  AFFE=_F( TOUT = 'OUI', MATER = MAT) )

# ON IMPOSE UN DEPLACEMENT AFFINE SUR TOUT LE BORD
# ---------------------------------------------------
FX = FORMULE(VALE='1.E-3*X',
             NOM_PARA=['X', 'Y'],)
FY = FORMULE(VALE='2.E-3*Y+1.E-3*X',
             NOM_PARA=['X', 'Y'],)


CHAR=AFFE_CHAR_MECA_F(MODELE=MO,
       DDL_IMPO=(
           #_F( GROUP_MA = 'FAC1',  DX=0., DY = 0., DZ = 0.),
           #_F( GROUP_MA = 'FAC3',  DX=1.E-3, DY=2.E-3, DZ=10.E-3),
            _F( GROUP_MA = ('FAC1',),  DX=FX, DY=FY,),
       ))



# 1 : CALCUL MECANIQUE :
#----------------------------------------
RESU1=MECA_STATIQUE( MODELE=MO, CHAM_MATER=CHAM_MAT, INFO=1,
                     EXCIT=( _F(CHARGE = CHAR)))



# 2 : CALCUL ET VALIDATION DE SIRO_ELEM SUR UNE FACE "EXTERNE" (FAC1)
#     ET UNE FACE "INTERNE" (FAC2) :
#-------------------------------------------------------------------------
CALC_CHAMP(reuse=RESU1,RESULTAT=RESU1,GROUP_MA=('RIGHT','UP','DOWN'),CONTRAINTE=('SIRO_ELEM'))
IMPR_RESU(FORMAT="RESULTAT",RESU=_F(RESULTAT=RESU1, NOM_CHAM='SIRO_ELEM',MAILLE=('M1','M5','M8')))

# 3 : ON VERIFIE QUE LE CALCUL SE PASSE BIEN SANS GROUP_MA :
#     DANS CE CAS ON FAIT LE CALCUL SUR TOUTES LES MAILLES LINEIQUES
#-------------------------------------------------------------------------
RESU2=CALC_CHAMP(RESULTAT=RESU1,CONTRAINTE=('SIRO_ELEM'))

SIRO = CREA_CHAMP(OPERATION = 'EXTR',
                    NOM_CHAM = 'SIRO_ELEM',
                    TYPE_CHAM = 'ELEM_SIEF_R',
                    RESULTAT = RESU2,
                    NUME_ORDRE = 1,
                    INFO=2,
                    )


#### USAGE TOUT_INI_ELEM AVEC CHAMPS NEUT_R ET NEUT_F POUR MODELISATION D_PLAN
#### ET MAILLES DE PEAU EN D_PLAN

MA = DEFI_GROUP(reuse = MA,
            MAILLAGE = MA,
            CREA_GROUP_MA = _F(NOM = 'LIGNE',
                               TYPE_MAILLE=('SEG2'),
                               TOUT='OUI',),)

CHAEL= CREA_CHAMP(TYPE_CHAM = 'ELEM_NEUT_R',
                    OPERATION = 'AFFE',
                    MODELE    = MO,
                    PROL_ZERO='OUI',
                    AFFE      = _F(GROUP_MA='LIGNE',
                                   NOM_CMP  = 'X1',
                                   VALE   = 1.0,),);

def fonc(sn,x1):
    a = sn*x1
    return a

FORM = FORMULE(VALE = 'fonc(SIG_N,X1)',
                     NOM_PARA=('SIG_N','X1',),fonc=fonc)

CHAELF = CREA_CHAMP(OPERATION = 'AFFE',
                  TYPE_CHAM = 'ELEM_NEUT_F',
                  MODELE = MO,
                  PROL_ZERO='OUI',
                  AFFE = _F(GROUP_MA='LIGNE',
                       NOM_CMP  = 'X1',
                       VALE_F   = FORM,),)

CHAELR = CREA_CHAMP(TYPE_CHAM = 'ELEM_NEUT_R',
       OPERATION = 'EVAL',
       CHAM_F    = CHAELF,
       CHAM_PARA = (SIRO,CHAEL),
       INFO=1,)

